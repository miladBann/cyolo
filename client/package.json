    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "axios-mock-adapter": "^1.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2"
